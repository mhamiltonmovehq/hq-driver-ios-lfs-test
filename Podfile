platform :ios, '8.0'

# Uncomment the next line if you're using Swift or would like to use dynamic frameworks
use_frameworks!

# Use existing workspace
workspace 'Survey'

def mm_pods
    # Pods for Mobile Mover Enterprise
    pod 'ImagePicker'
    pod 'Lightbox'
end

target 'Mobile Mover Enterprise' do
    mm_pods
end

target 'Mobile Mover' do
    mm_pods
end

